{
  "extends": [
    "next/babel",
    "next/core-web-vitals"
  ],
  "rules": {
    "newline-per-chained-call": [
      2,
      {
        "ignoreChainWithDepth": 4
      }
    ],
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "never"
    ],
    "react/react-in-jsx-scope": 0,
    "react/prop-types": "off"
  }
}
